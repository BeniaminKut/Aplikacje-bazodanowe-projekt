<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="708.0" maxWidth="301.0" prefHeight="708.0" prefWidth="301.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projekt4sem.DODAJController">
   <children>
      <Label text="Dodaj użytkownika">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label text="Podaj login:" />
      <TextField fx:id="login" />
      <Label text="Podaj hasło:" />
      <PasswordField fx:id="haslo" />
      <Label text="Wybierz rodzaj konta:" />
      <ChoiceBox fx:id="rodzaj" prefWidth="150.0" />
      <Label text="Podaj imie:" />
      <TextField fx:id="imie" />
      <Label text="Podaj nazwisko:" />
      <TextField fx:id="nazwisko" />
      <Label text="Podaj date urodzenia:" />
      <DatePicker fx:id="dataUrodzenia" />
      <Label text="Podaj ulice:" />
      <TextField fx:id="ulica" />
      <Label text="Podaj miejscowość:" />
      <TextField fx:id="miasto" />
      <Button mnemonicParsing="false" onAction="#onDodaj" text="DODAJ" />
   </children>
</VBox>
